<UserControl x:Class="OverWatch.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OverWatch"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button x:Name="button_go" Content="Go" HorizontalAlignment="Left" Margin="81,22,0,0" VerticalAlignment="Top" Width="142" Click="go" Height="76"/>
        <Button x:Name="button_stop" Content="Stop" HorizontalAlignment="Left" Margin="81,109,0,0" VerticalAlignment="Top" Width="142" Click="stop" Height="76"/>
        <TextBox x:Name="tb_loop" HorizontalAlignment="Left" Height="23" Margin="81,190,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="142" IsEnabled="False"/>

    </Grid>
</UserControl>
